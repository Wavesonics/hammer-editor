CREATE TABLE deleted_project (
		user_id INTEGER NOT NULL,
    	name TEXT NOT NULL,
    	uuid TEXT NOT NULL,
    	FOREIGN KEY(user_id) REFERENCES account(id)
);

getDeletedProjects:
SELECT *
FROM deleted_project
WHERE user_id = ?;

addDeletedProject:
INSERT INTO deleted_project(user_id, name, uuid)
VALUES(?, ?, ?);

removeDeletedProject:
DELETE FROM deleted_project
WHERE user_id = ? AND name = ?;

hasDeletedProject:
SELECT EXISTS(SELECT 1 FROM deleted_project WHERE user_id = ? AND uuid = ?);