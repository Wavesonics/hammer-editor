CREATE TABLE white_list(
    email TEXT NOT NULL PRIMARY KEY
);

isWhiteListed:
SELECT EXISTS
(SELECT 1
 FROM white_list
 WHERE email = ?);

addToWhiteList:
INSERT OR IGNORE
INTO white_list(email) VALUES (?);

removeFromWhiteList:
DELETE FROM white_list
WHERE email = ?;

getAll:
SELECT * FROM white_list;